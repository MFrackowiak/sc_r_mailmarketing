[run]
omit =
    mock/*
    mock.py
    tests/*
    web.py
    web/app.py
    email_client/app.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    @abstractmethod
show_missing = True
skip_covered = True